upstream django {
    server unix:///root/imobiliaria/mysite.sock; 
}

server {
    listen      8000;
    server_name example.com;
    charset     utf-8;

    client_max_body_size 75M; 

    location /media  {
        alias /root/imobiliaria/media; 
    }

    location /static {
        alias /root/imobiliaria/static;
    }

    location / {
        uwsgi_pass  django;
        include     /root/imobiliaria/uwsgi_params; 
    }
}